[supervisord]
logfile = /tmp/supervisord.log
logfile_maxbytes = 50MB
logfile_backups=10
loglevel = info
pidfile = /tmp/supervisord.pid
nodaemon = false
minfds = 1024
minprocs = 200
umask = 022
identifier = supervisor
directory = /tmp
nocleanup = true
childlogdir = /tmp
strip_ansi = false

[program:blindftp-ftp]
command=sudo python /var/www/data-diode/BlindFTP_0.37/bftp.py -s /var/www/data-diode/src/storage/app/files/ftp -a 192.168.101.2 -b -P 5 -p 36016 -u ftp -x
autostart=true
autorestart=true
user=www-data
numprocs=1
redirect_stderr=true
stdout_logfile=/var/www/data-diode/src/storage/app/bftp-diodein-ftp.log

[program:blindftp-apt]
command=sudo python /var/www/data-diode/BlindFTP_0.37/bftp.py -s /var/www/data-diode/src/storage/app/files/apt -a 192.168.101.2 -b -P 5 -p 36017 -u apt -x
autostart=true
autorestart=true
user=www-data
numprocs=1
redirect_stderr=true
stdout_logfile=/var/www/data-diode/src/storage/app/bftp-diodein-apt.log

[program:blindftp-pip]
command=sudo python /var/www/data-diode/BlindFTP_0.37/bftp.py -s /var/www/data-diode/src/storage/app/files/pip -a 192.168.101.2 -b -P 5 -p 36018 -u pip -x
autostart=true
autorestart=true
user=www-data
numprocs=1
redirect_stderr=true
stdout_logfile=/var/www/data-diode/src/storage/app/bftp-diodein-pip.log


[inet_http_server]         
port=127.0.0.1:9001

[supervisorctl]
serverurl=http://127.0.0.1:9001

[rpcinterface:supervisor]
supervisor.rpcinterface_factory = supervisor.rpcinterface:make_main_rpcinterface

